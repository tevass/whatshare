{
  "name": "@whatshare/server",
  "version": "1.0.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf dist .turbo",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:prod": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --config ./vitest.config.e2e.mts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.mts",
    "test:e2e:ui": "vitest --ui --config ./vitest.config.e2e.mts",
    "test:wwjs": "vitest run --config ./vitest.config.wwjs.mts",
    "test:wwjs:watch": "vitest --config ./vitest.config.wwjs.mts",
    "test:wwjs:ui": "vitest --ui --config ./vitest.config.wwjs.mts",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@nestjs/common": "^10.3.6",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.6",
    "@nestjs/platform-socket.io": "^10.3.6",
    "@nestjs/websockets": "^10.3.6",
    "@prisma/client": "5.9.0",
    "@whatshare/core": "workspace:^",
    "@whatshare/core-schemas": "workspace:^",
    "@whatshare/http-schemas": "workspace:^",
    "@whatshare/shared-schemas": "workspace:^",
    "@whatshare/ws-schemas": "workspace:^",
    "bcryptjs": "^2.4.3",
    "bson-objectid": "^2.0.4",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "mime-types": "^2.1.35",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "validator": "^13.11.0",
    "vcard4-tsm": "^1.0.0",
    "whatsapp-web.js": "^1.23.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.6",
    "@swc/core": "^1.4.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.30",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "@whatshare/eslint-config": "workspace:^",
    "@whatshare/tsconfig": "workspace:^",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "fs-extra": "^11.2.0",
    "mongodb": "^6.5.0",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "type-fest": "^4.14.0",
    "typescript": "^5.4.3",
    "unplugin-swc": "^1.4.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
